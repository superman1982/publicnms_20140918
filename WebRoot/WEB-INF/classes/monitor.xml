    <default>true</default>
    <enabled>true</enabled>
    <show_in_topo>true</show_in_topo>
    <nodetype>host</nodetype>
    <subentity>Utilization</subentity>
    <limenvalue0>60</limenvalue0>
    <limenvalue1>70</limenvalue1>
    <limenvalue2>80</limenvalue2>
    <time0>3</time0>
    <time1>3</time1>
